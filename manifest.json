{
  "name": "Demobile",
  "version": "0.0.4",
  "description": "Redirect browser to non-mobile version",
  "manifest_version": 2,
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "icons":{
		"16": "favicon-16x16.png",
		"48": "android-icon-48x48.png",
		"120": "apple-icon-120x120.png"
  },
  "permissions": [
    "webNavigation",
    "tabs"
  ]
}
